{
    "manifest_version": 3,
    "name": "Hyrox Tracker",
    "version": "1.0",
    "description": "Visual tracker for Hyrox races",
    "content_scripts": [
        {
            "matches": [
                "https://results.hyrox.com/season-*/?content=detail&fpid=search&pid=search&idp=*&lang=*&event=*"
            ],
            "js": ["content.js"],
            "css": ["styles.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["cologne-April-2025.jpg"],
            "matches": ["<all_urls>"]
        }
    ],
    "permissions": ["activeTab", "scripting"]
}
